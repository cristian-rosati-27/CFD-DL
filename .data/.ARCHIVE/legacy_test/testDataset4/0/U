FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 30);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (0 0 30);
    }

    outlet
    {
        type            zeroGradient;
    }

    rocket
    {
        type            noSlip;
    }

    walls
    {
        type            noSlip;
    }
}